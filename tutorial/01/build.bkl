<makefile>
  <include file='presets/simple.bkl' />
  <exe id='tutorial01' template='simple'>
    <!--
      GNUmakefile
    -->
    <if cond="FORMAT=='gnu'">
      <ldflags>
        `pkg-config --cflags libavformat libavcodec libswscale libavutil sdl`
      </ldflags>
      <cflags>
        `pkg-config --libs libavformat libavcodec libswscale libavutil sdl`
      </cflags>
    </if>
    <!--
      Sources Files:
    -->
    <sources>
      src/tutorial01.c
      src/queue.c
      src/list.c
      src/stack.c
    </sources>
    <include>
      ./include
    </include>
    <!--
      Headers Files:
    -->
    <headers>
      include/queue.h
      include/list.h
      include/stack.h
    </headers>
  </exe>
</makefile>
